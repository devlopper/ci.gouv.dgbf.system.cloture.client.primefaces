<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>indexView</from-outcome>
			<to-view-id>/private/operation/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Acts -->
		
		<navigation-case>
			<from-outcome>actListView</from-outcome>
			<to-view-id>/private/act/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actLockView</from-outcome>
			<to-view-id>/private/act/operate/lock.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actUnLockView</from-outcome>
			<to-view-id>/private/act/operate/unlock.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>actActLocksListView</from-outcome>
			<to-view-id>/private/act/lock/actlocks.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Imputations -->
		
		<navigation-case>
			<from-outcome>imputationListView</from-outcome>
			<to-view-id>/private/imputation/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- Operations -->
		
		<navigation-case>
			<from-outcome>operationListView</from-outcome>
			<to-view-id>/private/operation/list/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>operationCreateView</from-outcome>
			<to-view-id>/private/operation/create/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>operationReadView</from-outcome>
			<to-view-id>/private/operation/read/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>operationExecuteView</from-outcome>
			<to-view-id>/private/operation/execute/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>